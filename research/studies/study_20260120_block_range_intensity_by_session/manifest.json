  "metadata": {
    "title": "Block Range Intensity by Time-of-Day Session",
    "description": "Describes how score-outcome associations vary across fixed UTC time-of-day sessions (SESSION_A through SESSION_D)",
    "author": "@ovc-research",
    "created_at": "2026-01-20T00:00:00Z",
    "updated_at": "2026-01-20T00:00:00Z"
  },

  "canonical": {
    "release": "ovc-v0.1-spine",
    "feature_view": "derived.ovc_block_features_v0_1",
    "outcome_view": "derived.ovc_outcomes_v0_1"
  },

  "window": {
    "start": "2025-01-01T00:00:00Z",
    "end": "2026-01-01T00:00:00Z",
    "timezone": "UTC"
  },

  "inputs": {
    "instruments": ["GBPUSD"],
    "columns": {
      "features": ["block_id", "ts", "instrument", "rng"],
      "outcomes": ["fwd_ret_3", "mfe_3", "mae_3"]
    },
    "filters": {
      "description": "GBPUSD blocks in study window with non-null rng",
      "sql_fragment": "WHERE instrument = 'GBPUSD' AND ts >= '2025-01-01T00:00:00Z' AND ts < '2026-01-01T00:00:00Z' AND rng IS NOT NULL"
    }
  },

  "parameters": {
    "_comment": "All numeric/categorical parameters used in analysis",
    "sessions_utc": {
      "SESSION_A": {"start_hour": 0, "end_hour": 6, "description": "Asian session"},
      "SESSION_B": {"start_hour": 6, "end_hour": 12, "description": "London morning"},
      "SESSION_C": {"start_hour": 12, "end_hour": 18, "description": "NY / London overlap"},
      "SESSION_D": {"start_hour": 18, "end_hour": 24, "description": "NY evening"}
    },
    "score_buckets": [0, 10, 25, 50, 75, 90, 100],
    "normalization": "full-sample",
    "session_derivation": "EXTRACT(HOUR FROM ts) in UTC"
  },

  "outputs": {
    "score_name": "block_range_intensity",
    "score_version": "v1.0",
    "output_schema": {
      "columns": [
        "block_id",
        "session",
        "score_value",
        "score_bucket",
        "fwd_ret_3",
        "mfe_3",
        "mae_3"
      ]
    },
    "output_path": "research/outputs/study_20260120_block_range_intensity_by_session/"
  },

