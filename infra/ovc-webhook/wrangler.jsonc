
{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "ovc-webhook",
  "main": "src/index.ts",
  "compatibility_date": "2026-01-12",
  "compatibility_flags": ["nodejs_compat"],

  // Secrets:
  //   wrangler secret put OVC_TOKEN
  //   wrangler secret put DATABASE_URL
  // OVC_TOKEN is required for /tv_secure requests.

  "r2_buckets": [
    {
      "binding": "RAW_EVENTS",
      "bucket_name": "ovc-raw-events"
    }
  ]


	
}
